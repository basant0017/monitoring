{
  "uid": "home-monitoring",
  "title": "Monitoring Home",
  "style": "dark",
  "tags": ["monitoring"],
  "timezone": "browser",
  "schemaVersion": 38,
  "version": 1,
  "editable": true,
  "refresh": "30s",
  "panels": [
    {
      "type": "row",
      "title": "Global Health",
      "id": 10,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 0 },
      "collapsed": false,
      "panels": []
    },
    {
      "id": 11,
      "title": "Overall CPU Usage",
      "type": "stat",
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "gridPos": { "h": 4, "w": 6, "x": 0, "y": 1 },
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "decimals": 1,
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "orange", "value": 70 },
              { "color": "red", "value": 85 }
            ]
          }
        },
        "overrides": []
      },
      "options": {
        "colorMode": "background",
        "graphMode": "area",
        "justifyMode": "center",
        "reduceOptions": {
          "calcs": ["lastNotNull"],
          "fields": "",
          "values": false
        }
      },
      "targets": [
        {
          "refId": "A",
          "expr": "100 - (avg(rate(node_cpu_seconds_total{mode=\"idle\", job=~\"$job\"}[$__rate_interval])) * 100)"
        }
      ]
    },
    {
      "id": 12,
      "title": "Overall Memory Usage",
      "type": "stat",
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "gridPos": { "h": 4, "w": 6, "x": 6, "y": 1 },
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "decimals": 1,
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "orange", "value": 75 },
              { "color": "red", "value": 90 }
            ]
          }
        },
        "overrides": []
      },
      "options": {
        "colorMode": "background",
        "graphMode": "area",
        "justifyMode": "center",
        "reduceOptions": {
          "calcs": ["lastNotNull"],
          "fields": "",
          "values": false
        }
      },
      "targets": [
        {
          "refId": "A",
          "expr": "(1 - (sum(node_memory_MemAvailable_bytes{job=~\"$job\"}) / sum(node_memory_MemTotal_bytes{job=~\"$job\"}))) * 100"
        }
      ]
    },
    {
      "id": 13,
      "title": "Overall Disk Usage",
      "type": "stat",
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "gridPos": { "h": 4, "w": 6, "x": 12, "y": 1 },
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "decimals": 1,
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "orange", "value": 75 },
              { "color": "red", "value": 90 }
            ]
          }
        },
        "overrides": []
      },
      "options": {
        "colorMode": "background",
        "graphMode": "area",
        "justifyMode": "center",
        "reduceOptions": {
          "calcs": ["lastNotNull"],
          "fields": "",
          "values": false
        }
      },
      "targets": [
        {
          "refId": "A",
          "expr": "(1 - (sum(node_filesystem_avail_bytes{mountpoint=\"/\", fstype!~\"rootfs|tmpfs\", job=~\"$job\"}) / sum(node_filesystem_size_bytes{mountpoint=\"/\", fstype!~\"rootfs|tmpfs\", job=~\"$job\"}))) * 100"
        }
      ]
    },
    {
      "id": 14,
      "title": "Active Nodes",
      "type": "stat",
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "gridPos": { "h": 4, "w": 6, "x": 18, "y": 1 },
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "red", "value": 1 }
            ]
          }
        },
        "overrides": []
      },
      "options": {
        "colorMode": "background",
        "graphMode": "none",
        "justifyMode": "center",
        "reduceOptions": {
          "calcs": ["lastNotNull"],
          "fields": "",
          "values": false
        }
      },
      "targets": [
        {
          "refId": "A",
          "expr": "count(up{job=~\"$job\"} == 1)"
        }
      ]
    },
    {
      "type": "row",
      "title": "Hosts",
      "id": 20,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 5 },
      "collapsed": false,
      "panels": []
    },
    {
      "id": 21,
      "title": "Instances",
      "type": "table",
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "gridPos": { "h": 7, "w": 12, "x": 0, "y": 6 },
      "fieldConfig": {
        "defaults": { "color": { "mode": "palette-classic" } },
        "overrides": []
      },
      "options": {
        "showHeader": true,
        "sortBy": [{ "displayName": "CPU %", "desc": true }]
      },
      "targets": [
        {
          "refId": "A",
          "expr": "100 - (avg by (instance) (rate(node_cpu_seconds_total{mode=\"idle\", job=~\"$job\"}[$__rate_interval])) * 100)",
          "format": "table",
          "instant": true
        },
        {
          "refId": "B",
          "expr": "(1 - (node_memory_MemAvailable_bytes{job=~\"$job\"} / node_memory_MemTotal_bytes{job=~\"$job\"})) * 100",
          "format": "table",
          "instant": true
        },
        {
          "refId": "C",
          "expr": "(1 - (node_filesystem_avail_bytes{mountpoint=\"/\", fstype!~\"rootfs|tmpfs\", job=~\"$job\"} / node_filesystem_size_bytes{mountpoint=\"/\", fstype!~\"rootfs|tmpfs\", job=~\"$job\"})) * 100",
          "format": "table",
          "instant": true
        }
      ],
      "transformations": [
        { "id": "merge" },
        {
          "id": "organize",
          "options": {
            "renameByName": {
              "instance": "Instance",
              "Value": "CPU %",
              "Value 2": "Memory %",
              "Value 3": "Disk %"
            }
          }
        }
      ]
    },
    {
      "id": 22,
      "title": "Top Instances (CPU)",
      "type": "bargauge",
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "gridPos": { "h": 7, "w": 12, "x": 12, "y": 6 },
      "fieldConfig": {
        "defaults": { "unit": "percent" },
        "overrides": []
      },
      "options": {
        "displayMode": "basic",
        "orientation": "horizontal",
        "showUnfilled": false,
        "reduceOptions": {
          "calcs": ["lastNotNull"],
          "fields": "",
          "values": false
        }
      },
      "targets": [
        {
          "refId": "A",
          "expr": "topk(5, 100 - (avg by (instance) (rate(node_cpu_seconds_total{mode=\"idle\", job=~\"$job\"}[$__rate_interval])) * 100))",
          "format": "time_series"
        }
      ]
    },
    {
      "type": "row",
      "title": "Trends",
      "id": 30,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 13 },
      "collapsed": false,
      "panels": []
    },
    {
      "id": 31,
      "title": "CPU Usage",
      "type": "timeseries",
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "gridPos": { "h": 7, "w": 12, "x": 0, "y": 14 },
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "color": { "mode": "palette-classic" }
        },
        "overrides": []
      },
      "options": {
        "legend": { "displayMode": "table", "placement": "bottom" },
        "tooltip": { "mode": "multi" }
      },
      "targets": [
        {
          "refId": "A",
          "expr": "100 - (avg by (instance) (rate(node_cpu_seconds_total{mode=\"idle\", job=~\"$job\", instance=~\"$instance\"}[$__rate_interval])) * 100)",
          "legendFormat": "{{instance}}"
        }
      ]
    },
    {
      "id": 32,
      "title": "Memory Usage",
      "type": "timeseries",
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "gridPos": { "h": 7, "w": 12, "x": 12, "y": 14 },
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "color": { "mode": "palette-classic" }
        },
        "overrides": []
      },
      "options": {
        "legend": { "displayMode": "table", "placement": "bottom" },
        "tooltip": { "mode": "multi" }
      },
      "targets": [
        {
          "refId": "A",
          "expr": "(1 - (node_memory_MemAvailable_bytes{instance=~\"$instance\"} / node_memory_MemTotal_bytes{instance=~\"$instance\"})) * 100",
          "legendFormat": "{{instance}}"
        }
      ]
    },
    {
      "id": 33,
      "title": "Disk Usage (Root)",
      "type": "timeseries",
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "gridPos": { "h": 7, "w": 12, "x": 0, "y": 21 },
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "color": { "mode": "palette-classic" }
        },
        "overrides": []
      },
      "options": {
        "legend": { "displayMode": "table", "placement": "bottom" },
        "tooltip": { "mode": "multi" }
      },
      "targets": [
        {
          "refId": "A",
          "expr": "(1 - (node_filesystem_avail_bytes{instance=~\"$instance\", mountpoint=\"/\", fstype!~\"rootfs|tmpfs\"} / node_filesystem_size_bytes{instance=~\"$instance\", mountpoint=\"/\", fstype!~\"rootfs|tmpfs\"})) * 100",
          "legendFormat": "{{instance}}"
        }
      ]
    },
    {
      "id": 34,
      "title": "Network Traffic",
      "type": "timeseries",
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "gridPos": { "h": 7, "w": 12, "x": 12, "y": 21 },
      "fieldConfig": {
        "defaults": {
          "unit": "Bps",
          "color": { "mode": "palette-classic" }
        },
        "overrides": []
      },
      "options": {
        "legend": { "displayMode": "table", "placement": "bottom" },
        "tooltip": { "mode": "multi" }
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum by (instance) (rate(node_network_receive_bytes_total{instance=~\"$instance\", device!~\"lo|docker.*\"}[$__rate_interval]))",
          "legendFormat": "{{instance}} RX"
        },
        {
          "refId": "B",
          "expr": "sum by (instance) (rate(node_network_transmit_bytes_total{instance=~\"$instance\", device!~\"lo|docker.*\"}[$__rate_interval]))",
          "legendFormat": "{{instance}} TX"
        }
      ]
    },
    {
      "type": "row",
      "title": "System Deep Dive",
      "id": 50,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 28 },
      "collapsed": false,
      "panels": []
    },
    {
      "id": 51,
      "title": "Load Average (1m)",
      "type": "stat",
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "gridPos": { "h": 4, "w": 6, "x": 0, "y": 29 },
      "fieldConfig": {
        "defaults": {
          "unit": "none",
          "decimals": 2,
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "orange", "value": 2 },
              { "color": "red", "value": 4 }
            ]
          }
        },
        "overrides": []
      },
      "options": {
        "colorMode": "background",
        "graphMode": "none",
        "justifyMode": "center",
        "reduceOptions": {
          "calcs": ["lastNotNull"],
          "fields": "",
          "values": false
        }
      },
      "targets": [
        {
          "refId": "A",
          "expr": "avg(node_load1{job=~\"$job\", instance=~\"$instance\"})"
        }
      ]
    },
    {
      "id": 52,
      "title": "Swap Usage",
      "type": "stat",
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "gridPos": { "h": 4, "w": 6, "x": 6, "y": 29 },
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "decimals": 1,
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "orange", "value": 20 },
              { "color": "red", "value": 50 }
            ]
          }
        },
        "overrides": []
      },
      "options": {
        "colorMode": "background",
        "graphMode": "none",
        "justifyMode": "center",
        "reduceOptions": {
          "calcs": ["lastNotNull"],
          "fields": "",
          "values": false
        }
      },
      "targets": [
        {
          "refId": "A",
          "expr": "100 * (1 - (sum(node_memory_SwapFree_bytes{job=~\"$job\", instance=~\"$instance\"}) / sum(node_memory_SwapTotal_bytes{job=~\"$job\", instance=~\"$instance\"})))"
        }
      ]
    },
    {
      "id": 53,
      "title": "Host Uptime",
      "type": "stat",
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "gridPos": { "h": 4, "w": 6, "x": 12, "y": 29 },
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "decimals": 0,
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "red", "value": null },
              { "color": "orange", "value": 3600 },
              { "color": "green", "value": 86400 }
            ]
          }
        },
        "overrides": []
      },
      "options": {
        "colorMode": "background",
        "graphMode": "none",
        "justifyMode": "center",
        "reduceOptions": {
          "calcs": ["lastNotNull"],
          "fields": "",
          "values": false
        }
      },
      "targets": [
        {
          "refId": "A",
          "expr": "avg((node_time_seconds{job=~\"$job\", instance=~\"$instance\"} - node_boot_time_seconds{job=~\"$job\", instance=~\"$instance\"}))"
        }
      ]
    },
    {
      "id": 54,
      "title": "Processes (Running / Total / Blocked)",
      "type": "table",
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "gridPos": { "h": 4, "w": 6, "x": 18, "y": 29 },
      "fieldConfig": {
        "defaults": {
          "custom": { "align": "center" }
        },
        "overrides": []
      },
      "options": {
        "showHeader": true,
        "sortBy": [{ "displayName": "Running", "desc": true }]
      },
      "targets": [
        {
          "refId": "A",
          "expr": "node_procs_running{job=~\"$job\", instance=~\"$instance\"}",
          "format": "table",
          "instant": true
        },
        {
          "refId": "B",
          "expr": "node_procs_running{job=~\"$job\", instance=~\"$instance\"} + node_procs_blocked{job=~\"$job\", instance=~\"$instance\"}",
          "format": "table",
          "instant": true
        },
        {
          "refId": "C",
          "expr": "node_procs_blocked{job=~\"$job\", instance=~\"$instance\"}",
          "format": "table",
          "instant": true
        }
      ],
      "transformations": [
        { "id": "merge" },
        {
          "id": "organize",
          "options": {
            "renameByName": {
              "instance": "Instance",
              "Value": "Running",
              "Value 2": "Total",
              "Value 3": "Blocked"
            }
          }
        }
      ]
    },
    {
      "type": "row",
      "title": "Storage & I/O",
      "id": 55,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 33 },
      "collapsed": false,
      "panels": []
    },
    {
      "id": 56,
      "title": "Disk Usage by Mount",
      "type": "table",
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 34 },
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "orange", "value": 75 },
              { "color": "red", "value": 90 }
            ]
          }
        },
        "overrides": []
      },
      "options": {
        "showHeader": true,
        "sortBy": [{ "displayName": "Used %", "desc": true }]
      },
      "targets": [
        {
          "refId": "A",
          "expr": "(1 - (node_filesystem_avail_bytes{job=~\"$job\", instance=~\"$instance\", fstype!~\"rootfs|tmpfs|overlay\"} / node_filesystem_size_bytes{job=~\"$job\", instance=~\"$instance\", fstype!~\"rootfs|tmpfs|overlay\"})) * 100",
          "format": "table",
          "instant": true
        },
        {
          "refId": "B",
          "expr": "node_filesystem_size_bytes{job=~\"$job\", instance=~\"$instance\", fstype!~\"rootfs|tmpfs|overlay\"}",
          "format": "table",
          "instant": true
        }
      ],
      "transformations": [
        { "id": "merge" },
        {
          "id": "organize",
          "options": {
            "renameByName": {
              "instance": "Instance",
              "mountpoint": "Mount",
              "Value": "Used %",
              "Value 2": "Size (bytes)"
            }
          }
        }
      ]
    },
    {
      "id": 57,
      "title": "Inode Usage by Mount",
      "type": "table",
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 34 },
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "orange", "value": 75 },
              { "color": "red", "value": 90 }
            ]
          }
        },
        "overrides": []
      },
      "options": {
        "showHeader": true,
        "sortBy": [{ "displayName": "Inodes Used %", "desc": true }]
      },
      "targets": [
        {
          "refId": "A",
          "expr": "(1 - (node_filesystem_files_free{job=~\"$job\", instance=~\"$instance\", fstype!~\"rootfs|tmpfs|overlay\"} / node_filesystem_files{job=~\"$job\", instance=~\"$instance\", fstype!~\"rootfs|tmpfs|overlay\"})) * 100",
          "format": "table",
          "instant": true
        }
      ],
      "transformations": [
        {
          "id": "organize",
          "options": {
            "renameByName": {
              "instance": "Instance",
              "mountpoint": "Mount",
              "Value": "Inodes Used %"
            }
          }
        }
      ]
    },
    {
      "id": 58,
      "title": "Disk IOPS",
      "type": "timeseries",
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "gridPos": { "h": 7, "w": 12, "x": 0, "y": 42 },
      "fieldConfig": {
        "defaults": {
          "unit": "iops",
          "color": { "mode": "palette-classic" }
        },
        "overrides": []
      },
      "options": {
        "legend": { "displayMode": "table", "placement": "bottom" },
        "tooltip": { "mode": "multi" }
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum by (device) (rate(node_disk_reads_completed_total{job=~\"$job\", instance=~\"$instance\", device!~\"loop.*\"}[$__rate_interval]) + rate(node_disk_writes_completed_total{job=~\"$job\", instance=~\"$instance\", device!~\"loop.*\"}[$__rate_interval]))",
          "legendFormat": "{{device}}"
        }
      ]
    },
    {
      "id": 59,
      "title": "Disk Throughput",
      "type": "timeseries",
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "gridPos": { "h": 7, "w": 12, "x": 12, "y": 42 },
      "fieldConfig": {
        "defaults": {
          "unit": "Bps",
          "color": { "mode": "palette-classic" }
        },
        "overrides": []
      },
      "options": {
        "legend": { "displayMode": "table", "placement": "bottom" },
        "tooltip": { "mode": "multi" }
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum by (device) (rate(node_disk_read_bytes_total{job=~\"$job\", instance=~\"$instance\", device!~\"loop.*\"}[$__rate_interval]) + rate(node_disk_written_bytes_total{job=~\"$job\", instance=~\"$instance\", device!~\"loop.*\"}[$__rate_interval]))",
          "legendFormat": "{{device}}"
        }
      ]
    },
    {
      "id": 60,
      "title": "Disk Latency",
      "type": "timeseries",
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "gridPos": { "h": 7, "w": 24, "x": 0, "y": 49 },
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "decimals": 4,
          "color": { "mode": "palette-classic" }
        },
        "overrides": []
      },
      "options": {
        "legend": { "displayMode": "table", "placement": "bottom" },
        "tooltip": { "mode": "multi" }
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum by (device) (rate(node_disk_io_time_seconds_total{job=~\"$job\", instance=~\"$instance\", device!~\"loop.*\"}[$__rate_interval]) / clamp_min(rate(node_disk_reads_completed_total{job=~\"$job\", instance=~\"$instance\", device!~\"loop.*\"}[$__rate_interval]) + rate(node_disk_writes_completed_total{job=~\"$job\", instance=~\"$instance\", device!~\"loop.*\"}[$__rate_interval]), 1e-6))",
          "legendFormat": "{{device}}"
        }
      ]
    },
    {
      "type": "row",
      "title": "Network & Availability",
      "id": 61,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 56 },
      "collapsed": false,
      "panels": []
    },
    {
      "id": 62,
      "title": "Network In (All Instances)",
      "type": "stat",
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "gridPos": { "h": 4, "w": 6, "x": 0, "y": 57 },
      "fieldConfig": {
        "defaults": {
          "unit": "Bps",
